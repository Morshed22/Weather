# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'WeatherForeCast' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
    pod 'Alamofire', '~> 4.1'
    # pod 'Kingfisher', '~> 3.0'
    pod 'SwiftyJSON', '~> 3.1'
    # pod 'RxSwift', '~> 3.4'
    # pod 'RxCocoa', '~> 3.4'
    pod 'PKHUD', :git => 'https://github.com/pkluz/PKHUD.git', :branch => 'release/swift4'
    pod 'FacebookCore'
    pod 'FacebookLogin'
    pod 'Firebase/Auth'
    pod 'GoogleSignIn'
    
    
  # Pods for WeatherForeCast

  target 'WeatherForeCastTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire', '~> 4.1'
    pod 'SwiftyJSON', '~> 3.1'
    pod 'PKHUD', :git => 'https://github.com/pkluz/PKHUD.git', :branch => 'release/swift4'
  end

  target 'WeatherForeCastUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
